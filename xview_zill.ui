   <property name="styleSheet">
    <string notr="true">QPushButton:hover {
     /* 悬停状态：垂直渐变 + 白色文字 */
    background-color: qlineargradient(
        x1: 0, y1: 0,    /* 起点（顶部） */
        x2: 0, y2: 1,    /* 终点（底部） */
        stop: 0 rgb(110, 20, 150),    /* 顶部（亮紫） */
        stop: 0.5 rgb(85, 0, 127),    /* 中间（标准紫） */
        stop: 1 rgb(60, 0, 90)        /* 底部（深紫） */
    );
    color: white;  /* 悬停时文字变白 */
    border: 2px solid rgb(85, 0, 127);  /* 保持紫色边框 */
}
QPushButton:clear {
     /* 悬停状态：垂直渐变 + 白色文字 */
    background-color: qlineargradient(
        x1: 0, y1: 0,    /* 起点（顶部） */
        x2: 0, y2: 1,    /* 终点（底部） */
        stop: 0 rgb(110, 20, 150),    /* 顶部（亮紫） */
        stop: 0.5 rgb(85, 0, 127),    /* 中间（标准紫） */
        stop: 1 rgb(60, 0, 90)        /* 底部（深紫） */
    );
    color: white;  /* 悬停时文字变白 */
    border: 2px solid rgb(85, 0, 127);  /* 保持紫色边框 */
}
QPushButton:modify {
     /* 悬停状态：垂直渐变 + 白色文字 */
    background-color: qlineargradient(
        x1: 0, y1: 0,    /* 起点（顶部） */
        x2: 0, y2: 1,    /* 终点（底部） */
        stop: 0 rgb(110, 20, 150),    /* 顶部（亮紫） */
        stop: 0.5 rgb(85, 0, 127),    /* 中间（标准紫） */
        stop: 1 rgb(60, 0, 90)        /* 底部（深紫） */
    );
    color: white;  /* 悬停时文字变白 */
    border: 2px solid rgb(85, 0, 127);  /* 保持紫色边框 */
}
